pico-8 cartridge // http://www.pico-8.com
version 27
__lua__
--test por la partie qte
function _init()
	poke(0x5f2c,3)
	notes={}
end

function _update60()
	
	faire_note()
	for i=1,#notes do
		notes[i].x-=1
		if notes[i].x<=-3 then
			del (notes,notes[i])
		end
	end

end

function _draw()
	cls()
	spr(0,0,5,3,8)
	line(32,0,32,64,11)
	for i=1,#notes do
		n=notes[i]
		circfill(n.x,n.y,n.r,n.c)
		if n.x<=35 then
			circfill(n.x,n.y,n.r,9)
		end
	
	end

end

function faire_note()

	if btnp(1) then
		n={}
		n.x=64
		n.y=50
		n.c=7
		n.r=2
		add(notes,n)
	end


end




