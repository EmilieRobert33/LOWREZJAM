pico-8 cartridge // http://www.pico-8.com
version 27
__lua__
--test por la partie qte
function _init()
	poke(0x5f2c,3)
	notes={}
end

function _update60()
	
	faire_note()
	for i=1,#notes do
		if (notes[i]!=nill) then
			notes[i].x-=1
			if (notes[i].x<-3) del(notes,notes[i])
		end
	end
	

end

function _draw()
	cls()
	print (#notes)
	spr(0,15,4,4,4)
	line(32,50,32,64,11)
	for i=1,#notes do
		local n=notes[i]
		print(n.s,n.x,n.y)
--		pset(n.x+3,n.y+3,9)
	
	end

end

function faire_note()

	if btnp(2) then
		local n={x=64,y=55,s="⬆️"}
		add(notes,n)
	end

	if btnp(3) then
		local n={x=64,y=55,s="⬇️"}
		add(notes,n)
	end
	if btnp(1) then
		local n={x=64,y=55,s="➡️"}
		add(notes,n)
	end
	if btnp(0) then
		local n={x=64,y=55,s="⬅️"}
		add(notes,n)
	end





end

